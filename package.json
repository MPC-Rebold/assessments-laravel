{
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "reformat": "npm run reformat:prettier && npm run reformat:pint",
    "reformat:prettier": "prettier --write .",
    "reformat:pint": "php ./vendor/bin/pint",
    "test": "php artisan test",
    "test:bare": "php artisan test --exclude-group=sync",
    "test:sync": "php artisan test --group=sync",
    "serve": "php artisan serve",
    "dev": "vite",
    "build": "vite build"
  },
  "devDependencies": {
    "@shufo/prettier-plugin-blade": "^1.14.0",
    "@tailwindcss/forms": "^0.5.2",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.4",
    "laravel-vite-plugin": "^1.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
